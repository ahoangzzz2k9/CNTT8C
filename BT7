#include <stdio.h>
int main() {
    int m, n;
    int a[10][10];

    scanf("%d%d", &m, &n);     // Nhap sa dong, cot

    for(int i = 0; i < m; i++)
        for(int j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    int min = a[0][0];         // Gia su phan tu Ä‘au nho nhat
    for(int i = 0; i < m; i++)
        for(int j = 0; j < n; j++)
            if(a[i][j] < min)  // So sanh
                min = a[i][j]; // Cap nhat min

    printf("Gia tri nho nhat = %d", min);
    return 0;
}
