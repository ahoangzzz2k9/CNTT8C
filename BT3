#include <stdio.h>

int main()
{
    int m, n;              // so dong va so cot
    int a[10][10];         // khai bao mang

    printf("Nhap so dong: "); scanf("%d", &m);
    printf("Nhap so cot: "); scanf("%d", &n);
    // Nhap mang
    for (int i = 0; i < m; i++)
    {
        for (int j = 0; j < n; j++)
        {
            printf("a[%d][%d] = ", i, j); scanf("%d", &a[i][j]);
        }
    }

    // Tinh tong tung dong
    for (int i = 0; i < m; i++)
    {
        int tongDong = 0;        // khoi tao tong dong
        for (int j = 0; j < n; j++)
        {
            tongDong += a[i][j]; // cong tung phan tu cua dong
        }
        printf("Tong dong %d = %d\n", i, tongDong);
    }

    return 0;
}
