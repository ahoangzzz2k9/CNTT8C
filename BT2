#include <stdio.h>

int main()
{
    int m, n;              // khai bao so dong, so cot
    int a[10][10];         // khai bao mang
    int tong = 0;          // bien luu tong

    printf("Nhap so dong: "); scanf("%d", &m);
    printf("Nhap so cot: "); scanf("%d", &n);

    // Nhap du lieu cho mang
    for (int i = 0; i < m; i++)
    {
        for (int j = 0; j < n; j++)
        {
            printf("a[%d][%d] = ", i, j);
            scanf("%d", &a[i][j]);
            tong += a[i][j];   // cong don vao tong
        }
    }

    printf("\nTong cac phan tu = %d", tong);

    return 0;
}
