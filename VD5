#include <stdio.h>

int main()
{
    int i, j;

    /* Phan tren cua trai tim */
    for (i = 1; i <= 3; i++)
    {
        for (j = 1; j <= 17; j++)
        {
            if ((j >= 5 - i && j <= 5 + i) ||
                (j >= 13 - i && j <= 13 + i))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

    /* Phan duoi cua trai tim */
    for (i = 9; i >= 1; i--)
    {
        for (j = 1; j <= 17; j++)
        {
            if (j >= 9 - i && j <= 9 + i)
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

    return 0;
}
