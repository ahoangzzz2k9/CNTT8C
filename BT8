#include <stdio.h>

int main() {
    int n;
    int a[10][10];
    int tong = 0;              // Bien luu tong

    scanf("%d", &n);           // Nhap kich thuoc ma tran vuong

    for(int i = 0; i < n; i++)
        for(int j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    for(int i = 0; i < n; i++)
        tong += a[i][i];       // Cong phan tu Ä‘uong cheo chinh

    printf("Tong duong cheo chinh = %d", tong);
    return 0;
}
